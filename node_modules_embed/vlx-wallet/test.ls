require! {
    \./Wallet.js : { default: Wallet }
    \./Crypto/index.js : { default: VelasCrypto }
    \bip39
}

to-callback = (p, cb)->
    p.then (res)->
        cb null, res
    p.catch (res)->
        cb res
index = 1
err, vc <- to-callback VelasCrypto.init!
seedBuffer = bip39.mnemonicToSeed("asfd asdf asdf asdf ")
seed = seedBuffer.toString('hex')
res  = vc.keysGen.fromSeed(seed, 'm/' + index + '\'')

address = res.to-wallet!.Base58Address
console.log address
